import java.util.*;

//TODO: Find a way to remove this. This is just so the module precedence pass
//can traverse all aspects 
aspect OMCollectAspectNames {

	public void ASTNode.collectAspectNames() {
		for (int i = 0 ; i < getNumChild(); i++) {
			getChild(i).collectAspectNames();
		}
	}
	public void AspectDecl.collectAspectNames() {
		getOMAbcExtension().moduleStruct.aspect_names.add(this.fullName());
		super.collectAspectNames();
	}
	

}