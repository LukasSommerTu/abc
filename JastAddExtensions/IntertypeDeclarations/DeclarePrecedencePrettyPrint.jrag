aspect DeclarePrecedencePrettyPrint {
  public void DeclarePrecedence.toString(StringBuffer s) {
    s.append(indent());
    s.append("declare precedence: ");
    for(int i = 0; i < getNumPattern(); i++) {
      if(i != 0) s.append(", ");
      getPattern(i).toString(s);
    }
    s.append(";\n");
  }
}
