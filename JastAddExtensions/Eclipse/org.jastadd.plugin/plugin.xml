         </part>
      </actionSetPartAssociation>
   </extension>
   
   <extension
         point="org.eclipse.ui.contexts">
      <context
            name="JastAddEditing Scope"
            description="JastAdd Editing Scope"
            parentId="org.eclipse.ui.textEditorScope"
            id="org.jastadd.plugin.jastAddEditorScope">
      </context>
   </extension>
   -->
</plugin>
