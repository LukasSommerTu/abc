aspect ExtractMethod {
	public MethodDecl Block.extractMethod(String visibility, String name, int begin, int end) {
		Block blk = doExtractBlock(begin, end);
		AnonymousMethod am = blk.doWrapIntoClosure();
		am = am.convert();
		MethodDecl md = am.doExtractToMethod(visibility, name);
		return md;
	}
	
	public MethodDecl Block.doExtractMethod(String visibility, String name, int begin, int end) {
		return extractMethod(visibility, name, begin, end);
	}
}